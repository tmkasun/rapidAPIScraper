                                            {
                                                "name": "last activity",
                                                "operation": "XPATH",
                                                "selector": "//a[contains(@href, 'lastact')]"
                                            },
                                            {
                                                "name": "action1",
                                                "operation": "RUN_SCRIPT",
                                                "selector": "//a[contains(., 'An issue filtering related models')]",
