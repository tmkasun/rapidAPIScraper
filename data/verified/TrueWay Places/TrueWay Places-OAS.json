    "tags": [],
    "paths": {
        "/FindPlacesNearby": {
            "get": {
                "parameters": [
                    {
                        "name": "language",
                        "in": "query",
                        "required": false,
                        "description": "The language in which to return results",
                        "schema": {
                            "type": "string",
                            "example": "en"
                        }
                    },
                    {
                        "name": "radius",
                        "in": "query",
                        "required": false,
                        "description": "The distance (in meters) within which to return results. Max = 10000 m",
                        "schema": {
                            "type": "number",
                            "example": "150"
                        }
                    },
                    {
                        "name": "type",
                        "in": "query",
                        "required": false,
                        "description": "The type of places that are returned",
                        "schema": {
                            "type": "string",
                            "example": "cafe"
                        }
                    },
                    {
                        "name": "location",
                        "in": "query",
                        "required": true,
                        "description": "The latitude/longitude around which to retrieve places",
                        "schema": {
                            "type": "string",
                            "example": "37.783366,-122.402325"
                        }
                    }
                ],
                "operationId": "FindPlacesNearby",
                "description": "Search for nearby places",
                "responses": {
                    "200": {
                        "description": ""
                    }
                }
            }
        },
        "/FindPlaceByText": {
            "get": {
                "parameters": [
                    {
                        "name": "bounds",
                        "in": "query",
                        "required": false,
                        "description": "Rectangular bounds (south,west,north,east)",
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "language",
                        "in": "query",
                        "required": false,
                        "description": "The language in which to return results",
                        "schema": {
                            "type": "string",
                            "example": "en"
                        }
                    },
                    {
                        "name": "text",
                        "in": "query",
                        "required": true,
                        "description": "A text string on which to search",
                        "schema": {
                            "type": "string",
                            "example": "Children's Creativity Museum"
                        }
                    }
                ],
                "operationId": "FindPlaceByText",
                "description": "Search for places by text string",
                "responses": {
                    "200": {
                        "description": ""
                    }
                }
            }
        }
    }
