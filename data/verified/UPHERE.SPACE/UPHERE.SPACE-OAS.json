        "/satellite/{number}/location": {
            "get": {
                "tags": [
                    "satellites"
                ],
                "parameters": [
                    {
                        "name": "units",
                        "in": "query",
                        "required": false,
                        "description": "Units for height and speed values.  Options are:\n- imperial\n- metric",
