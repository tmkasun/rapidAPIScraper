                            "type": "string"
                        }
                    },
                    {
                        "name": "lat",
                        "in": "query",
                        "required": false,
                        "description": "Latitude to get satellite visibility",
                        "schema": {
                            "type": "number",
                            "default": "47.6484346"
                        }
                    },
                    {
                        "name": "lng",
                        "in": "query",
                        "required": false,
                        "description": "Longitude to get satellite visibility",
                        "schema": {
                            "type": "number",
                            "default": "122.374199"
                        }
                    },
                    {
                        "name": "number",
                        "in": "path",
                        "required": true,
                        "description": "NORAD TLE number",
                        "schema": {
                            "type": "number",
                            "example": "20580"
                        }
                    }
                ],
                "externalDocs": {
