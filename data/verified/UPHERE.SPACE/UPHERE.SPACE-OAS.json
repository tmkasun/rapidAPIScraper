                    "url": "https://uphere.space/development/api/documentation"
                },
                "operationId": "Location",
                "description": "Current location by NORAD TLE number",
                "responses": {
                    "200": {
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "coordinates": {
                                            "items": {
