        "/api/v2/head-to-head/{id}": {
            "get": {
                "tags": [
                    "Match Statistics"
                ],
                "parameters": [
                    {
                        "name": "limit",
                        "in": "query",
                        "required": false,
                        "description": "Limit the search to only X previous encounters. (max is 10)",
                        "schema": {
                            "type": "number",
                            "example": "10"
                        }
                    },
                    {
                        "name": "id",
                        "in": "path",
                        "required": true,
                        "description": "(use predictions endpoint to get list of IDs)",
                        "schema": {
                            "type": "number",
                            "example": "81930"
                        }
                    }
                ],
                "operationId": "Head To Head",
                "description": "Shows head to head stats and previous encounters for the home and away team of an upcoming match.",
